{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/UserManagement.Web/bin/Debug/net7.0/UserManagement.Web.dll",
            "args": [],
            "cwd": "${workspaceFolder}/UserManagement.Web",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/UserManagement.Web/Views"
            }
        }
    ]
}
